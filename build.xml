<project>
  <target name="clean">
	<delete dir="build"/>
  </target>

  <target name="compile">
	<mkdir dir="build/classes"/>
	<javac srcdir="./" destdir="build/classes"/>
  </target>

  <target name="run">
	<java classpath="build/classes/" classname="WebServer" fork="true"/>
  </target>

</project>
